<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UISEOForm>
    <label>	  
      <title>Title: </title>
      <description>Description: </description>
      <keywords>Keywords: </keywords>
      <language>Language: </language>
      <robots>Robots: </robots>
      <sitemap>Sitemap: </sitemap>
      <sitemapvisible>Visible in Sitemap</sitemapvisible>
	    <isInherited>Inherited from parent page</isInherited>
      <priority>Priority: </priority>
      <frequency>Frequency: </frequency> 
      <select-language>Select a language</select-language>
      <option>
        <Always>Always</Always>
        <Hourly>Hourly</Hourly>
        <Daily>Daily</Daily>
        <Weekly>Weekly</Weekly>
        <Monthly>Monthly</Monthly>
        <Yearly>Yearly</Yearly>
        <Never>Never</Never>
      </option> 
    </label>
    <tip>
		<priority>Valid values range from 0.0 to 1.0</priority>
	</tip>	 	  
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <repository-exception>An unexpected error occurred while saving the page metadata.</repository-exception> 	
    </msg>
	  <help> 
		  <priority>Valid values range from 0.0 to 1.0.</priority>
		  <description>Help searchers to let them know what the following page has a link with what they are looking for.</description>
		  <keywords>Most important terms in the page separated by a comma.</keywords>		  
	  </help>		  
  </UISEOForm>
  <UISEOPopupWindow>
  	<title>
      <UISEOForm>SEO Management</UISEOForm>
  	</title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  	<title>
      <seo>SEO Management</seo>
  	</title>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>The value of the Priority field must be in the numerical format. The valid values range is from 0.0 to 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>  
  
</bundle>
